<launch>
  <param name="use_sim_time" value="false"/>

  <node name="cartographer_node" pkg="cartographer_ros" type="cartographer_node" args="-configuration_directory $(find omni_slam)/config -configuration_basename localization.lua -load_state_filename $(find omni_slam)/maps/map_207.pbstream"
    output="screen">
  </node>

  <node name="cartographer_occupancy_grid_node" pkg="cartographer_ros" type="cartographer_occupancy_grid_node" args="-resolution 0.05"/>
</launch>
